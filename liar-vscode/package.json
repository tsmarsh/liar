{
  "name": "liar-lang",
  "displayName": "liar",
  "description": "Syntax highlighting for the liar programming language",
  "version": "0.1.0",
  "publisher": "liar-lang",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": ["Programming Languages"],
  "contributes": {
    "languages": [
      {
        "id": "liar",
        "aliases": ["liar", "Liar"],
        "extensions": [".liar"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "liar",
        "scopeName": "source.liar",
        "path": "./syntaxes/liar.tmLanguage.json"
      }
    ]
  }
}
